{
  "manifest_version": 2,
  "name": "ROBLOX Forum FilterGuard",
  "version": "0.1",
  "description": "Bypass the roblox forum filter with weird characters.",
  "author": "Kodran",
  "icons": { 	"16": "icon16.png",
				"48": "icon48.png",
				"128": "icon128.png" },
  "browser_action": {
	"default_icon": "icon48.png"
  },
  "content_scripts": [
  {
    "matches": [
      "https://forum.roblox.com/Forum/*",
	  "http://forum.roblox.com/Forum/*"
    ],
    "js": ["content.js"]
  }
]
}